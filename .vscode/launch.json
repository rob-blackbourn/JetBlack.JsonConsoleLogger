  "version": "0.2.0",
  "configurations": [
    {
      "name": "Example",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build Example",
      "program": "${workspaceFolder}/examples/simple/bin/Debug/netcoreapp3.0/Example.dll",
      "args": [],
      "cwd": "${workspaceFolder}/examples/simple",
      "console": "integratedTerminal",
      "stopAtEntry": false,
      "justMyCode": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
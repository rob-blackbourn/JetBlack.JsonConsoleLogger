{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build Example",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/examples/simple/Example.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "publish Example",
      "command": "dotnet",
      "type": "process",
      "args": [
        "publish",
        "${workspaceFolder}/examples/simple/Example.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "watch Example",
      "command": "dotnet",
      "type": "process",
      "args": [
        "watch",
        "run",
        "${workspaceFolder}/examples/simple/Example.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build JetBlack.JsonConsoleLogger",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/JetBlack.JsonConsoleLogger/JetBlack.JsonConsoleLogger.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "publish JetBlack.JsonConsoleLogger",
      "command": "dotnet",
      "type": "process",
      "args": [
        "publish",
        "${workspaceFolder}/JetBlack.JsonConsoleLogger/JetBlack.JsonConsoleLogger.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "watch JetBlack.JsonConsoleLogger",
      "command": "dotnet",
      "type": "process",
      "args": [
        "watch",
        "run",
        "${workspaceFolder}/JetBlack.JsonConsoleLogger/JetBlack.JsonConsoleLogger.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
  ]
}